{
	"name": "apache-ignite-log-pretty-printer",
	"displayName": "Apache Ignite Log Pretty Printer",
	"description": "Pretty prints apache ignite logs",
	"publisher": "dany74q",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.ignitePrettyPrint"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.ignitePrettyPrint",
				"title": "Ignite Pretty Print"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
